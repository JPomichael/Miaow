@{
    ViewBag.Title = "Login";
}
<div class="login_div">
    <form name="login-form" id="login-form" onsubmit="return false;">
    <ul>
        <li style="padding-top: 8px;">
            <label>
                Email：</label>
            <input type="text" name="email" id="email" maxlength="45" class="btn_input" value=""
                onkeydown="if(event.keyCode==13)event.keyCode=9" />
        </li>
        <li class="bd_t1" style="padding: 7px 0 6px 0;">
            <label>
                密码：</label>
            <input type="password" name="pwd" id="pwd" maxlength="18" class="btn_input" value=""
                onkeydown="if(event.keyCode==13)login()" />
        </li>
        <li class="bd_t1">
            <label>
                &nbsp;</label>
            <input type="button" value="登 录" class="btn_login" onclick="login()">
            <input type="reset" value="重 填" class="btn_login">
        </li>
    </ul>
    </form>
</div>
