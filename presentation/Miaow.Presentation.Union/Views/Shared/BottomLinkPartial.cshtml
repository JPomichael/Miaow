@{
    var hotelVouchData = iPow.Presentation.Union.iPowPreUnionEngine.Current.Resolve<
        iPow.Service.Union.Service.IHeadVouchDataService>();
}
<span class="imgLink"></span><span class="textLink">
    @{
        IQueryable<iPow.Application.Union.Dto.HotelBottomLinkDto> hl = hotelVouchData.GetHotelBottomLinks();
        if (hl != null)
        {
            foreach (var item in hl)
            {
        <a href="@(item.Url)" target="_blank" class="aText" title="@(item.Title)">@(item.Title)</a>
            }
        }
    }
</span>