<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-2.5.3.js")" type="text/javascript"></script>
    <style type="text/css">
        *
        {
            margin: 0;
            padding: 0;
        }
        a
        {
            text-decoration: none;
        }
        img
        {
            border: none;
        }
        .ipowRegist
        {
            width: 1003px;
            margin: 0 auto;
            margin-top:20px;
        }
        .ipowRegistLeft
        {
            float: left;
            margin-left: 5px;
            _display: inline;
        }
        .ipowRegistRight
        {
            float: right;
            width: 328px;
            height: 302px;
            background: url(/images/ipowRegBgRight.jpg ) no-repeat;
            margin-right: 5px;
            _display: inline;
            margin-bottom:20px;
        }
        .ipowRegistRight ul
        {
            display: inline-block;
            border-bottom: 1px solid #bebcbd;
            width: 313px;
            margin-left: 4px;
            float: left;
            _display: inline;
            margin-top: 10px;
            padding-left: 5px;
            margin-bottom: 15px;
        }
        .ipowRegistRight ul a
        {
            float: left;
            display: block;
            width: 99px;
            height: 32px;
            line-height: 32px;
            color: #89898a;
            background: url(/images/ipowRegBtn.jpg) no-repeat;
            position: relative;
            top: 1px;
            text-align: center;
            margin-right: 5px;
            _display: inline;
            font-weight: bold;
            font-size: 14px;
        }
        .ipowRegistRight ul a.ipowRegistACurr
        {
            background: url(/images/ipowRegBtnCurr.jpg) no-repeat;
            color: #333;
        }
        .ipowRegistRight span
        {
            display: inline-block;
            color: #000;
            float: left;
            margin: 15px 0 0 40px;
            _display: inline;
            font-size: 12px;
        }
        .ipowRegistRight span input
        {
            height: 20px;
        }
        .ipowRegistRight span.ipowRegistEnter
        {
            margin-top: 15px;
            border-bottom: 1px solid #bebcbd;
            padding-bottom: 40px;
            width: 230px;
        }
        .ipowRegistRight span.ipowRegistEnter .ipowRegistEnterBtn
        {
            position: relative;
            top: 10px;
            margin: 0 10px 0 48px;
        }
        .ipowRegistRight span.ipowRegistEnter a
        {
            font-weight: normal;
            color: #00b1e7;
        }
        .ipowRegistWB
        {
            display: inline-block;
            margin: 20px 0 0 25px;
            float: left;
            _display: inline;
        }
        .ipowRegistWB a
        {
            margin-right: 10px;
        }
        .ipowRegistTabMain
        {
            width: 100%;
            height: 163px;
            overflow: hidden;
            position: relative;
        }
        .ipowRegistTab
        {
            display: none;
            position: absolute;
            left: 0;
            top: 0;
        }
    </style>
</head>
<body style="background:#ffd778;">
    <div class="ipowRegist">
        <div class="ipowRegistLeft">
            <img src="/images/ipowRegBg.jpg">
        </div>
        @RenderBody()
        
    </div>
    <div class="pageBottom">
            @Html.Partial("BottomPartial")
        </div>
</body>
</html>
